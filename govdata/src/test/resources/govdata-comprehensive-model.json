{
  "version": "1.0",
  "defaultSchema": "sec",
  "schemas": [
    {
      "name": "sec",
      "type": "custom",
      "factory": "org.apache.calcite.adapter.govdata.GovDataSchemaFactory",
      "operand": {
        "dataSource": "sec",
        "directory": "${SEC_CACHE_DIR:/tmp/govdata-comprehensive-test/sec-data}",
        "useMockData": false,
        "autoDownload": true,
        "startYear": 2023,
        "endYear": 2024,
        "filingTypes": ["10-K"],
        "cikList": ["0000320193", "0000789019"],
        "fetchStockPrices": true,
        "extractFootnotes": true,
        "extractMDA": true,
        "extractRiskFactors": false,
        "textSimilarity": {
          "enabled": true,
          "embeddingDimension": 256,
          "provider": "local",
          "maxTextLength": 8192
        },
        "executionEngine": "DUCKDB",
        "tableNameCasing": "SMART_CASING",
        "columnNameCasing": "SMART_CASING"
      }
    },
    {
      "name": "econ",
      "type": "custom",
      "factory": "org.apache.calcite.adapter.govdata.GovDataSchemaFactory",
      "operand": {
        "dataSource": "econ",
        "cacheDirectory": "${ECON_CACHE_DIR:/tmp/govdata-comprehensive-test/econ-cache}",
        "blsApiKey": "${BLS_API_KEY:}",
        "fredApiKey": "${FRED_API_KEY:}",
        "beaApiKey": "${BEA_API_KEY:}",
        "updateFrequency": "daily",
        "historicalDepth": "2 years",
        "enabledSources": ["bls", "treasury", "worldbank", "fred", "bea"],
        "startYear": 2023,
        "endYear": 2024,
        "limitedMode": true,
        "blsSeriesConfig": {
          "employment": ["UNRATE", "PAYEMS", "CIVPART"],
          "inflation": ["CPIAUCSL", "CPILFESL"],
          "wages": ["CES0500000003", "CES0500000008"]
        },
        "fredSeriesConfig": {
          "keyIndicators": ["GDPC1", "FEDFUNDS", "DGS10", "M2SL", "DEXUSEU"],
          "maxSeries": 50
        },
        "worldBankConfig": {
          "countries": ["USA", "CHN", "JPN", "DEU", "GBR"],
          "indicators": ["NY.GDP.MKTP.CD", "FP.CPI.TOTL.ZG", "SL.UEM.TOTL.ZS"]
        },
        "beaConfig": {
          "tables": ["NIPA-1.1.1", "NIPA-1.1.5"],
          "regions": ["US", "CA", "NY", "TX"]
        },
        "executionEngine": "DUCKDB",
        "tableNameCasing": "SMART_CASING",
        "columnNameCasing": "SMART_CASING"
      }
    },
    {
      "name": "geo",
      "type": "custom",
      "factory": "org.apache.calcite.adapter.govdata.GovDataSchemaFactory",
      "operand": {
        "dataSource": "geo",
        "cacheDirectory": "${GEO_CACHE_DIR:/tmp/govdata-comprehensive-test/geo-cache}",
        "autoDownload": true,
        "dataYear": 2024,
        "enabledSources": ["tiger", "census", "hud"],
        "limitedMode": true,
        "states": ["CA", "NY", "TX"],
        "hudDataTypes": ["ZIP_COUNTY", "ZIP_TRACT", "ZIP_CBSA"],
        "tigerDataTypes": ["STATE", "COUNTY"],
        "censusApiKey": "${CENSUS_API_KEY:}",
        "hudUsername": "${HUD_USERNAME:}",
        "hudPassword": "${HUD_PASSWORD:}",
        "hudToken": "${HUD_TOKEN:}",
        "executionEngine": "DUCKDB",
        "tableNameCasing": "SMART_CASING",
        "columnNameCasing": "SMART_CASING"
      }
    }
  ]
}