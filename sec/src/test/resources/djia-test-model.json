{
  "version": "1.0",
  "defaultSchema": "sec",
  "schemas": [{
    "name": "sec",
    "type": "custom",
    "factory": "org.apache.calcite.adapter.sec.SecSchemaFactory",
    "operand": {
      "directory": "${SEC_TEST_DATA_DIR:-build/test-data/sec}/dji-real-test",
      "ciks": ["_DJIA_CONSTITUENTS"],
      "autoDownload": true,
      "startYear": 2014,
      "endYear": 2024,
      "executionEngine": "duckdb",
      "testMode": true,
      "ephemeralCache": true,
      "sec.fallback.enabled": false
    }
  }]
}