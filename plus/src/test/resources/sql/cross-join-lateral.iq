!set outputformat mysql

# Checks whether CROSS JOIN LATERAL works
SELECT SC.email EMAIL, TF.TYPE, TF.CODEVALUE
FROM ENHANCED.SIMPLE_CUSTOMER SC
CROSS JOIN LATERAL TABLE(AUX.CODES(SC.email)) TF limit 6;
