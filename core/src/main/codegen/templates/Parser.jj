SqlLiteral Natural() :
{
}
{
    (
        <NATURAL> { return SqlLiteral.createBoolean(true, getPos()); }
    |
        { return SqlLiteral.createBoolean(false, getPos()); }
    )
}

SqlLiteral JoinType() :
{
    JoinType joinType;
