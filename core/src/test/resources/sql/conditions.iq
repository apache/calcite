!use catchall
!set outputformat mysql

# OR test

with tmp(a, b) as (
  values (1, 1), (1, 0), (1, cast(null as int))
       , (0, 1), (0, 0), (0, cast(null as int))
       , (cast(null as int), 1), (cast(null as int), 0), (cast(null as int), cast(null as int)))
select *
  from tmp
 where a = 1 or b = 1
 order by 1, 2;

+---+---+
| A | B |
+---+---+
| 0 | 1 |
| 1 | 0 |
| 1 | 1 |
| 1 |   |
|   | 1 |
+---+---+
(5 rows)

!ok

with tmp(a, b) as (
  values (1, 1), (1, 0), (1, cast(null as int))
       , (0, 1), (0, 0), (0, cast(null as int))
       , (cast(null as int), 1), (cast(null as int), 0), (cast(null as int), cast(null as int)))
select *
  from tmp
 where not (a = 1 or b = 1)
 order by 1, 2;

+---+---+
| A | B |
+---+---+
| 0 | 0 |
+---+---+
(1 row)

!ok

