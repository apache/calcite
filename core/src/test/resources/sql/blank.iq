!use blank
!set outputformat mysql

create table foo (i int not null, j int);
(-1 rows modified)

!update
insert into foo values (1, 0);
(1 row modified)

!update
insert into foo values (0, 2);
(1 row modified)

!update
select * from foo;
+---+---+
| I | J |
+---+---+
| 0 | 2 |
| 1 | 0 |
+---+---+
(2 rows)

!ok

# Correlated non-equi IN
select * from foo as f where i in (
  select j from foo where i > f.i);
+---+---+
| I | J |
+---+---+
| 0 | 2 |
+---+---+
(1 row)

!ok

