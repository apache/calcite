    LogicalProject(ENAME=[$1], EXPR$1=[RAND_INTEGER(2)])
      LogicalTableScan(table=[[CATALOG, SALES, EMP]])
!sub-plan "NONE"
LogicalProject(ENAME=[$0])
  LogicalSort(sort0=[$1], dir0=[ASC])
    LogicalProject(ENAME=[$1], EXPR$1=[RAND_INTEGER(2)])
      LogicalTableScan(table=[[CATALOG, SALES, EMP]])
!sub-plan "NONE, SORT_REMOVE_CONSTANT_KEYS"

