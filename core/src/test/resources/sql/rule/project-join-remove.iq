SELECT e.deptno, d1.deptno
FROM emp e
LEFT JOIN dept d1 ON e.deptno = d1.deptno
LEFT JOIN dept d2 ON e.deptno = d2.deptno;
+--------+--------+
| DEPTNO | DEPTNO |
+--------+--------+
|     10 |     10 |
|     10 |     10 |
|     10 |     10 |
|     20 |     20 |
|     20 |     20 |
|     20 |     20 |
|     20 |     20 |
|     20 |     20 |
|     30 |     30 |
|     30 |     30 |
|     30 |     30 |
|     30 |     30 |
|     30 |     30 |
|     30 |     30 |
+--------+--------+
(14 rows)

!ok
LogicalProject(DEPTNO=[$7], DEPTNO0=[$9])
  LogicalJoin(condition=[=($7, $11)], joinType=[left])
