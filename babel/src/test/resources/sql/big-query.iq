| parsed_time |
+-------------+
| 15:00:00    |
+-------------+
(1 row)

!ok

SELECT PARSE_TIME('%I:%M:%S %p', '2:23:38 pm') AS parsed_time;
+-------------+
| parsed_time |
+-------------+
| 14:23:38    |
+-------------+
(1 row)

!ok


