    DATE_TRUNC(DATETIME "2008-12-25 15:30:00", MONTH) AS datetime_result;
timestamp_result TIMESTAMP_WITH_LOCAL_TIME_ZONE NOT NULL
datetime_result TIMESTAMP NOT NULL
!type

