#
WITH items AS (
  SELECT 'IFNULL(null, 5)' question, IFNULL(null, 5) AS answer UNION ALL
  SELECT 'IFNULL(2, 1)', IFNULL(2, 1) UNION ALL
  SELECT "IFNULL('b', null)", IFNULL('b', null) UNION ALL
  SELECT 'IFNULL(null, null)', IFNULL(null, null)
)
SELECT * FROM items;
+--------------------+--------+
