select 1.0 % 2;
+--------+
| EXPR$0 |
+--------+
|    1.0 |
+--------+
(1 row)

!ok

# [CALCITE-7310] Support the syntax SELECT * EXCLUDE(columns)
select * exclude(empno, ename, job, mgr) from emp limit 1;
